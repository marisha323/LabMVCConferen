@{
}
@model IEnumerable<LabMVCConferen.Models.User>
<table class="table">
    <thead class="thead-dark">
        <tr>
           
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">Phone</th>
        </tr>
    </thead>
    <tbody>
      @if (Model != null)
      {
            @foreach (LabMVCConferen.Models.User item in Model)
            {
            <tr>
               
                <td>@item.Name</td>
                <td>@item.Email</td>
                <td>@item.Phone</td>
            </tr>
        }
      }
       
    </tbody>
</table>